<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ang.dao.AcoountMapper">
    <select id="findAll" resultType="acoount">
        select * from acoount;
    </select>

    <insert id="save" parameterType="acoount">
        insert into acoount values(#{id},#{name},#{money})
    </insert>
</mapper>